<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"   android:background="#FFFFFF"
    android:orientation="horizontal" >
<!-- baodan -->
    <TextView
        android:id="@+id/ids"    android:layout_marginLeft="15dip"
        android:layout_width="wrap_content" android:textSize="20sp"   android:textColor="#000000"
        android:layout_height="wrap_content" />
<!-- bao ren-->
    <TextView
        android:id="@+id/people"     android:layout_marginLeft="65dip" android:textColor="#019858"
        android:layout_width="wrap_content" android:textSize="20sp"
        android:layout_height="wrap_content" />
<!-- by bao ren-->
    <TextView
        android:id="@+id/bypeople"  android:layout_marginLeft="65dip" android:textColor="#019858"
        android:layout_width="wrap_content" android:textSize="20sp"
        android:layout_height="wrap_content" />
<!-- state-->
    <TextView
        android:id="@+id/cardstate"  android:layout_marginLeft="85dip" android:textColor="#000000"
        android:layout_width="wrap_content" android:textSize="20sp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/time"  android:layout_marginLeft="125dip" android:textColor="#000000"
        android:layout_width="wrap_content" android:textSize="20sp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/baofei"  android:layout_marginLeft="125dip" android:textColor="#000000"
        android:layout_width="wrap_content" android:textSize="20sp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/starttime"  android:layout_marginLeft="95dip" android:textColor="#000000"
        android:layout_width="wrap_content" android:textSize="20sp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/sf"  android:layout_marginLeft="95dip" android:textColor="#019858"
        android:layout_width="wrap_content" android:textSize="20sp"
        android:layout_height="wrap_content" />

</LinearLayout>