<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:background="#000000">
    <RelativeLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="6dp"
        android:background="@drawable/id_tanchu_title_bg"
        >
        <TextView android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="分享到"
            android:textColor="#ffffff"
            android:textSize="20sp"
            android:layout_centerInParent="true"
            />
        <ImageButton android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/id_tanchu_close"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:id="@+id/closeWindowBtn1"
            />
    </RelativeLayout>
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#ffffff"
        >
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="#ffffff"
        >
        <LinearLayout android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:id="@+id/sharelayout1"
            android:layout_weight="1"
            android:padding="10dp"
            >
            <Button android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableTop="@drawable/fenxiang_ico1"
                android:clickable="false"
                android:text="新浪微博"
	            android:textColor="#000000"
	            android:textSize="16sp"
	            android:drawablePadding="10dp"
                />
        </LinearLayout>
        <View android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@drawable/id_tanchu_line"
            />
        <LinearLayout android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_weight="1"
            android:id="@+id/sharelayout6"
            android:padding="10dp"
            >
             <Button android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableTop="@drawable/fenxiang_ico4"
                android:clickable="false"
                android:text="微信"
	            android:textColor="#000000"
	            android:textSize="16sp"
	            android:drawablePadding="10dp"
                />
        </LinearLayout>
        
        <View android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@drawable/id_tanchu_line"
            />
        <LinearLayout android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_weight="1"
            android:id="@+id/sharelayout3"
            android:padding="10dp"
            >
             <Button android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableTop="@drawable/fenxiang_ico3"
                android:clickable="false"
                android:text="QQ"
	            android:textColor="#000000"
	            android:textSize="16sp"
	            android:drawablePadding="10dp"
                />
        </LinearLayout>
    </LinearLayout>
    <View android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@drawable/soso_zc_line"
            />
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="#ffffff"
        >
        <LinearLayout android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_weight="1"
            android:id="@+id/sharelayout4"
            android:padding="10dp"
            >
            <Button android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableTop="@drawable/id_fenxiang_ico4"
                android:clickable="false"
                android:text="邮件"
	            android:textColor="#000000"
	            android:textSize="16sp"
	            android:drawablePadding="10dp"
                />
        </LinearLayout>
        
        <View android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@drawable/id_tanchu_line"
            />
        <LinearLayout android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_weight="1"
            android:id="@+id/sharelayout2"
            android:padding="10dp"
            >
             <Button android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableTop="@drawable/fenxiang_ico2"
                android:clickable="false"
                android:text="短信"
	            android:textColor="#000000"
	            android:textSize="16sp"
	            android:drawablePadding="10dp"
                />
        </LinearLayout>
        <View android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@drawable/id_tanchu_line"
            />
         <LinearLayout android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_weight="1"
            android:id="@+id/sharelayout5"
            android:padding="10dp"
            android:visibility="invisible"
            >
             <Button android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:drawableTop="@drawable/fenxiang_ico5"
                android:clickable="false"
                android:text="二维码"
	            android:textColor="#000000"
	            android:textSize="16sp"
	            android:drawablePadding="10dp"
                />
        </LinearLayout>
    </LinearLayout>
    </LinearLayout>
</LinearLayout>